{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "noEmit": true,
    "paths": {
      "@styles/*" : ["./src/elements/*"],
      "@comp/*" : ["./src/components/*"],
      "@utils/*" : ["./src/Utils/*"],
      "@hooks/*" : ["./src/Hooks/*"],
      "@form/*" : ["./src/components/Forms/*"],
      "@SVG/*" : ["./src/components/atoms/SVG/*"],

    }
  },
  "include": [
    "src"
  ]
}
